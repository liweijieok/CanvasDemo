<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="canvas_demo.liweijie.github.com.canvasdemo">

  <application
      android:allowBackup="true"
      android:icon="@mipmap/ic_launcher"
      android:label="@string/app_name"
      android:roundIcon="@mipmap/ic_launcher_round"
      android:supportsRtl="true"
      android:theme="@style/AppTheme">
    <activity android:name=".MainActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>

        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>
    <activity android:name=".CanvasTranslateActivity" android:label="Translate"/>
    <activity android:name=".CanvasScaleActivity" android:label="Scale"/>
    <activity android:name=".CanvasRotateActivity" android:label="Rotate"/>
  </application>

</manifest>